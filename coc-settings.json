{
    "languageserver": {
        "clangd": {
            "command": "clangd",
            "filetypes": ["c", "cpp", "objc", "objcpp"]
        },
        "rust-analyzer": {
            "command": "rust-analyzer",
            "filetypes": ["rust"],
			"rustup.overrideCommand": "rustup run stable cargo",
  			"procMacro.enable": true,
  			"cargo.features": null

        }
    }
}

